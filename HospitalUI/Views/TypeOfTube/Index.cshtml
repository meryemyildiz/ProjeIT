@using EntityLayer.Concrete
@model List<TypeOfTube>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h3>Tüp Sayfası</h3>
<br />
<!DOCTYPE html>
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />

<table class="table table-hover">
    <tr>
        <th>ID</th>
        <th>TÜP CİNSİ</th>
    </tr>
          
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.TubeId</td>
            <td>@item.TubeName</td>
            <td><a href="/TypeOfTube/DeleteTube/@item.TubeId" class="btn btn-danger">SİL</a></td>
            <td><a href="/TypeOfTube/UpdateTube/@item.TubeId" class="btn btn-warning">GÜNCELLE</a></td>
        </tr>
       
    }
</table>
<a href="/TypeOfTube/AddTube" class="btn btn-info">Yeni Tüp Girişi</a>

