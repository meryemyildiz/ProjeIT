@model EntityLayer.Concrete.ProjectStatus
@{
    ViewBag.Title = "AddProjectStatus";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<link href="~/Content/bootstrap.min.css" rel="stylesheet" />

@using (Html.BeginForm("AddProjectStatus", "ProjectStatus", FormMethod.Post))
{
    <div class="form-group">
        @Html.Label("Proje Durumu")
        @Html.TextBoxFor(x => x.ProStatus, new { @class = "form-control" })
        @Html.ValidationMessageFor(x => x.ProStatus)
        <button class="btn btn-info">Kaydet</button>

    </div>
}